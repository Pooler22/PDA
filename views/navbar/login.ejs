<md-sidenav layout="column" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')">

  <% if (session.authenticated && session.User.admin) { %>
    <md-button href="/course/new">
      <%= __('add-course') %>
    </md-button>
    <md-button href="/user">
      <%= __('users') %>
    </md-button>
    <md-button href="/user/new">
      <%= __('add-users') %>
    </md-button>
    <% } %>



      <% if (session.authenticated) { %>
        <md-button class="logout form-group">
          <a href="/user/show/<%= session.User.id %>">Twój profil</a>
          <a href="/session/destroy">Wyloguj się</a>
        </md-button>
        <% } %>
</md-sidenav>
