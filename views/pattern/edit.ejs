<form action="/board/update/<%= board.id %>" method="POST" class="form-rename">
  <h2>Zmiana nazwy tablicy: <%= board.name %></h2>
  <input value="<%= board.name %>" name="name" type="text" class="form-control"/>
  <input type="hidden" name="_csrf" value="<%= _csrf %>">
  <button type="submit" class="btn btn-default">Zmień nazwę</button>
</form>
